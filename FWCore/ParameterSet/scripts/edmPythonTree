#! /usr/bin/env python

from FWCore.ParameterSet.TreeCrawler import getImportTree
import sys, os

try:
    filename = sys.argv[1]
except:
    parser.error("Please specify a python config file to inspect")

sys.path.append(os.environ["PWD"])
